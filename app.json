{
  "expo": {
    "name": "Baccarat Scoreboard",
    "slug": "baccarat-scoreboard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.qiyang2017.baccaratapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.qiyang2017.baccaratapp"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "jsEngine": "hermes",
    "EX_DEV_CLIENT_NETWORK_INSPECTOR": "true",
    "newArchEnabled": "true"
  },
  "@react-native-async-storage/async-storage": "^2.1.2"
} 